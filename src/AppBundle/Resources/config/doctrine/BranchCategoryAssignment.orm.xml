<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="AppBundle\Entity\BranchCategoryAssignment" 
        table="branch_category_assignments"
        repository-class="AppBundle\Repository\BranchCategoryAssignmentRepository">
        
        <id name="branch" association-key="true" />
        <id name="category" association-key="true" />
        
        <many-to-one field="branch" target-entity="Branch" />
        <many-to-one field="category" target-entity="Category" />
        
    </entity>
    
</doctrine-mapping>