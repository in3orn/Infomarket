{% extends 'benchmark/index.html.twig' %}
	
{% block content %}

	<div class="container">
		{% block errorsBlock %}
			{% include 'common/errors.html.twig' %}
		{% endblock %}
		
		{% block preFormBlock %}
		{% endblock %}
		
		{% block formBlock %}
		{% form_theme form 'form/fields.html.twig' %}
		
		<div class="row">
			<div class="col-xs-12">
			{{ form_start(form) }}
				{% block moreFields1 %}
			    {% endblock %}
			
			    {% block headerFields %}
			    {% endblock %}
			    
			    {% block simpleHeaderFields %}
			    {% endblock %}
			    
			    {% block flagFields %}
			    {% endblock %}
			    
			    {% block contentFields %}
			    {% endblock %}
			
				{{ form_errors(form) }}
			
				{% block actions %}
			    {% endblock %}
			{{ form_end(form) }}
			</div>
		</div>
			    
		{% endblock %}
	</div>
	
	<div class="vr-space-40"></div>
	
	<div class="row">
		{% if entry.id %}
			{% block assignments %}
	    	{% endblock %}
    	{% endif %}
	</div>
	
	{% block moreContent %}
	{% endblock %}
	
{% endblock %}
