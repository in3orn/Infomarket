{% block moreFields3 %}
	
	{% include 'common/fields/text_field.html.twig' with 
	{ 'label': 'label.icon'|trans, 'value': entry.icon } %}
	
	{% include 'common/fields/color_field.html.twig' with 
	{ 'label': 'label.color'|trans, 'value': entry.color } %}
	
	{% include 'common/fields/text_field.html.twig' with 
	{ 'label': 'label.orderNumber'|trans, 'value': entry.orderNumber } %}
	
	<div class="row">
		<div class="col-xs-12">
			<h4>{{ 'label.content'|trans }}</h4>
		</div>
		<div class="col-xs-12">
			{{ entry.content | raw }}
		</div>
	</div>
	
{% endblock %}