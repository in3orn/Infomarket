{% extends 'admin/base/show.html.twig' %}

{% set mainPath = 'benchmark_field' %}
{% set title = 'title.benchmarkFields'|trans %}
{% set gender = '' %}

{% block mainFields1 %}	
	{% include 'common/fields/simple_field.html.twig' with { 'label': 'label.category', 
		'value': entry.category, 'object': 'category', 'routeId': entry.id } %}
{% endblock %}

{% block moreFields2 %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.valueType', 'value': benchmarkFieldValueTypeName(entry.valueType)|trans } %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.valueNumber', 'value': entry.valueNumber } %}
	
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.fieldName', 'value': entry.fieldName } %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.fieldType', 'value': benchmarkFieldFieldTypeName(entry.fieldType)|trans } %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.fieldNumber', 'value': entry.fieldNumber } %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.showField', 'value': entry.showField } %}
	
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.filterName', 'value': entry.filterName } %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.filterType', 'value': benchmarkFieldFilterTypeName(entry.filterType)|trans } %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.filterNumber', 'value': entry.filterNumber } %}
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.showFilter', 'value': entry.showFilter } %}
	
	{% include 'common/fields/text_field.html.twig' with { 'label': 'label.benchmarkField.decimalPlaces', 'value': entry.decimalPlaces } %}
{% endblock %}