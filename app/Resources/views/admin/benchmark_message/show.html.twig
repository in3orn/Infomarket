{% extends 'admin/simple/show.html.twig' %}

{% set mainPath = 'benchmark_field' %}
{% set title = 'title.benchmarkFields'|trans %}


{% block headerFields %}
	{% include 'common/fields/header_field.html.twig' with 
	{ value: entry.name ~ ' ' ~ entry.product.brand.name } %}
{% endblock %}

{% block contentFields %}
	{% include 'admin/benchmark_message/components/message_content.twig' %}
	
	{% for entry in entry.children %}
		{% include 'admin/benchmark_message/components/message_content.twig' %}
	{% endfor %}
	
	{% block messageForm %}
		{% include 'admin/benchmark_message/components/message_form.twig' %}
	{% endblock %}
	
{% endblock %}