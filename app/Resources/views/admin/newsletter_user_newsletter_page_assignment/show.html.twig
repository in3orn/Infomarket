{% extends 'admin/base/show.html.twig' %}

{% set mainPath = 'newsletter_user_newsletter_page_assignment' %}
{% set title = 'title.newsletter_user_newsletter_page_assignments'|trans %}
{% set gender = '' %}

{% block mainFields1 %}	
	{% include 'common/fields/simple_field.html.twig' with 
	{ 'label': 'label.newsletterUser', 'value': entry.newsletterUser,
		'object': 'newsletter_user', 'routeId': entry.id } %}
		
	{% include 'common/fields/simple_field.html.twig' with 
	{ 'label': 'label.newsletterPage', 'value': entry.newsletterPage,
		'object': 'newsletter_page', 'routeId': entry.id } %}
		
	{% include 'common/fields/text_field.html.twig' with 
		{ 'label': 'label.newsletterUserNewsletterPageAssignment.state', 'value': newsletterUserNewsletterPageAssignmentStateName(entry.state)|trans } %}
		
	{% include 'common/fields/date_field.html.twig' with 
		{ 'label': 'label.newsletterUserNewsletterPageAssignment.processingTime', 'value': entry.processingTime, format: 'i:s' } %}
{% endblock %}